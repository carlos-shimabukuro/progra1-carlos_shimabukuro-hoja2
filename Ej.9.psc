Algoritmo Notas
	//datos de entrada
	Escribir "Escribir nota"
	Leer n
	//Restricciones
	//calculo
	si n<=20 & n>=19
		Entonces
		Escribir "Nota A"
	SiNo
		si n<=18 & n>=16
			Entonces
			Escribir "Nota B"
		SiNo
			si n<=15 & n>=13
				Entonces
				Escribir "Nota C"
			SiNo
				si n<=12 & n>=10
					Entonces
					Escribir "Nota D"
				SiNo
					si n<=9 & n>=1
						Entonces
						Escribir "Nota E"
						
					FinSi
				FinSi
				
			FinSi
		FinSi
	FinSi
	

	
FinAlgoritmo
